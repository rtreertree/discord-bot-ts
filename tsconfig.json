    "rootDir": "./src/",
    "outDir": "./prod/",
    "target": "ESNext",                               
    "module": "commonjs",                                
    "esModuleInterop": true,        
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "skipLibCheck": true,
    "sourceMap": false, 
    "baseUrl": "./"
  },
  "include": [
    "./*/*.ts",
  ],

}
